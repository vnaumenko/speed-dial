{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["DOM", "ES2020", "ESNext"],
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "useDefineForClassFields": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
}
